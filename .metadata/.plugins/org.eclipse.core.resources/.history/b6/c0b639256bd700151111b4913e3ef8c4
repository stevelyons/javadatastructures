package project1;

public class Counter
{
	private int count;
	
	Counter()
	{
	} // end of constructor
	
	Counter(int count)
	{
		this.count = count;
	} // end of constructor
	
	void setCount(int number)
	{
		this.count = number;
	} // end of setCount
	
	void incrementCount()
	{
		(this.count)++;
	}
} // end of Counter
