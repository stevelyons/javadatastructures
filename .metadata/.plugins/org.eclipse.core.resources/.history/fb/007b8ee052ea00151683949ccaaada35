	public final void clear()
	{
		firstNode = null;
		length = 0;
	} // end clear
	
	
	priate Node getNodeAt(int givenPosition)
	{
		
	} // end of getNodeAt

	private class Node
	{
		
	} // end of Node
} // end of LList
