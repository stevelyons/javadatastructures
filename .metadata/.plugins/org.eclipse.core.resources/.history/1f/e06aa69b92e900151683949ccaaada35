package chapter6;

public class LList <T> implements ListInterface<T>
{
	private Node firstNode;    // reference to first node
	private int length;   	   // number of entries in list
	
	public LList()
	{
		clear();
	} // end default constructor
	
	public final void clear()
	{
		firstNode = null;
		length = 0;
	} // end clear

}
